for kputc
Pseudocode

//look at uart_csreg *pucsr = (struct uart_csreg *))xB8000300 //default location for UART I/O

unsigned char lookAddress = pucsr + pucsr.UART_LSR_TEMT;

while(lookAddress == 0)
{
  //do nothing, wait for TEMT to be 1 (both registers are empty)
}

//put char into serial port

kcheckc

//return 1 if character is available on serial port

for kgetc()
Pseudocode
//look at the unget buffer for a character

volatile struct pl011_uart_csreg *regptr;

/* Pointer to the UART control and status register */
regptr = (struct pl0111_uart_csreg *)0x20201000;

char *flagReg = regptr + (regptr->fr);

while ((regptr->PL011_FR_TXFE) != 1)
{
  
}

*flagReg = c;

return c;
